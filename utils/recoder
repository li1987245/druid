select a.tagvalues,b.tagvalues,count(1) as count from pay_way a
join (select sex.idtype,sex.id,sex.tagvalues from sex left semi join
(select tagvalues from sex group by tagvalues having count(*)>25000) tmp on sex.tagvalues=tmp.tagvalues) b
on (a.id=b.id and a.idtype=b.idtype) group by a.tagvalues,b.tagvalues;

