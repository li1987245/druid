match (n)-[*]-(m) detach delete n,m
merge (a:Person {gid:"09f8d02f-70ea-11e7-afc1-5435303b8159"})
on create set a.phone =13999999910
with a
match (b:Person {phone:13999999910})
with b
where b.gid <> "09f8d02f-70ea-11e7-afc1-5435303b8158"
create (a)-[:phone]->(b)


